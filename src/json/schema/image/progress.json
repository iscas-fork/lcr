{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "type": "object",
    "properties": {
        "image": {
            "type": "string"
        },
        "progresses": {
            "type": "array", 
            "items": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "message": {
                        "type": "string"
                    },
                    "total": {
                        "type": "int64"
                    },
                    "current": {
                        "type": "int64"
                    },
                    "start": {
                        "type": "int64"
                    },
                    "unit": {
                        "type": "string"
                    }
                }
            }
        }
    }
}
