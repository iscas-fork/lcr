{
    "cwd": "/home/user",
    "args": ["arg1", "arg2"],
    "consoleSize": {
        "height": 25,
        "width": 80
    },
    "env": [
        "key1=value1",
        "key2=value2"
    ],
    "terminal": true,
    "user": {
        "uid": 1000,
        "gid": 1000,
        "additionalGids": [1001, 1002],
        "username": "user"
    },
    "capabilities": {
        "bounding": ["CAP_CHOWN", "CAP_DAC_OVERRIDE"],
        "permitted": ["CAP_NET_BIND_SERVICE"],
        "effective": ["CAP_SYS_ADMIN"],
        "inheritable": ["CAP_SYS_CHROOT"],
        "ambient": ["CAP_NET_RAW"]
    },
    "apparmorProfile": "profile",
    "oomScoreAdj": 0,
    "selinuxLabel": "system_u:system_r:unconfined_t:s0",
    "noNewPrivileges": false,
    "rlimits": [
        {
            "hard": 200,
            "soft": 100,
            "type": "RLIMIT_NOFILE"
        }
    ]
}
